  "codename": "??? ???",
  "description": "A virtual tabletop for board games and RPGs.",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=14.0.0"
  },
  "author": "Markus Leupold-LÃ¶wenthal",
  "homepage": "https://github.com/ludus-leonis/FreeBeeGee",
  "repository": {
    "type": "git",
